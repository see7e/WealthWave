{
    "version": 2,
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python3.9",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "run.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9"
    }
}